class Solution:
    def subsetsWithDup(self, nums: List[int]) -> List[List[int]]:
        n = len(nums)
        nums.sort()
        ans, sol = [], []

        def backtrack(i):
            if i == n:
                ans.append(sol[:])
                return

            # include nums[i]
            sol.append(nums[i])
            backtrack(i + 1)
            sol.pop()

            # skip duplicates
            while i < n - 1 and nums[i] == nums[i + 1]:
                i += 1

            # exclude nums[i]
            backtrack(i + 1)

        backtrack(0)
        return ans
